class Solution:
    def mySqrt(self, x: int) -> int:
        start=0
        end=x
        while start<=end:
            mid=(start+end)//2
            sq=mid*mid
            if sq==x:
                return mid
            elif sq>x:
                end=mid-1
            elif sq<x:
                start=mid+1
        return end
        
        

        
