class Solution:
    def isIsomorphic(self, s: str, t: str) -> bool:
        mapping={}
        visited=set()
        for i in range(len(s)):
            if s[i] not in mapping:
                if t[i] in visited:
                    return False
                else:
                    mapping.update([(s[i],t[i])])
                    visited.add(t[i])
            else:
                if mapping[s[i]]!=t[i]:
                    return False
        return True


